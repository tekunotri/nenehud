"UnlitGeneric"
{
	"$basetexture" "vgui/replay/thumbnails/numbersql" // alt number, the font accurate one looks a bit weird
	"$translucent" 1
	"$vertexcolor" 1
	"$no_fullbright" 1
	"$ignorez" 1
	"$speed" 0
	Proxies {
		PlayerSpeed {
			scale 1
			resultVar $speed
		}
		Clamp {
			min 0
			max 3500
			srcVar1 $speed
			resultVar $frame
		}
	}
}
